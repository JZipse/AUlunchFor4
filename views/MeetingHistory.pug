html
    head
        title AULF4
        link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css")
        style   
            include ../CSS/style2.css
    body
    div
        nav(class="navbar navbar-expand navbar-light bg-light")
            div(class="container-fluid")
                div(class="navbar-header")
                    ul(class="nav navbar-nav")
                        li(class = "nav-item")
                            a(class="nav-link" href="/customerPage") Home
                        li(class = "nav-item")
                            a(class="nav-link" href="https://aurora.edu/") Aurora University

        h1 AU Lunch For 4
        .container
            .row 
                .col
                    table(style='width:90%', border='1', class="table center")
                        tr
                            th Meeting Date
                            th Meeting ID
                            th member1
                            th member2
                            th member3
                            th member4
                            th member5
                            for m in meetings
                                tr
                                    td #{m.meetDate}
                                    td #{m.meetingID}
                                    td #{m.member1}
                                    td #{m.member2}
                                    td #{m.member3}
                                    td #{m.member4}
                                    td #{m.member5}
            .row
                .col-2
                    button(type="button" class="btn btn-primary" onclick="history.back()") Back 